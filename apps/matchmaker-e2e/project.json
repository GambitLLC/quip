{
  "name": "matchmaker-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": [
    "socket-server",
    "matchmaker-frontend",
    "matchmaker-backend",
    "director",
    "matchfunction"
  ],
  "targets": {
    "build-setup": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "go build -o build/e2e/bin/e2e-setup -tags setup apps/matchmaker-e2e/setup.go",
          "go build -o build/e2e/bin/e2e-validate -tags validate apps/matchmaker-e2e/validate.go",
          "make build/e2e/bin/minimatch build/e2e/bin/default-evaluator build/e2e/bin/synchronizer"
        ]
      }
    },
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/matchmaker-e2e/jest.config.ts",
        "passWithNoTests": true,
        "silent": false
      },
      "dependsOn": ["^build", "build-setup"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/matchmaker-e2e/**/*.{js,ts}"]
      }
    }
  }
}
