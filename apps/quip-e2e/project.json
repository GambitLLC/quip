{
  "name": "quip-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": [
    "socket-server",
    "matchmaker-frontend",
    "director",
    "matchfunction"
  ],
  "targets": {
    "build-setup": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "go build -o dist/apps/e2e-setup -tags setup apps/quip-e2e/setup.go",
          "go build -o dist/apps/e2e-validate -tags validate apps/quip-e2e/validate.go"
        ]
      }
    },
    "e2e": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/quip-e2e/jest.config.ts",
        "passWithNoTests": true,
        "silent": false
      },
      "dependsOn": ["^build", "build-setup"]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/quip-e2e/**/*.{js,ts}"]
      }
    }
  }
}
