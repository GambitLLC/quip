{
  "name": "matchmaker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/matchmaker",
  "targets": {
    "make": {
      "command": "make matchmaker-protos"
    },
    "build": {
      "executor": "@nx-go/nx-go:build",
      "dependsOn": ["^make", "make"],
      "options": {
        "outputPath": "dist/packages/matchmaker",
        "main": "packages/matchmaker/main.go"
      }
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "main": "packages/matchmaker/main.go"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    }
  },
  "tags": []
}
