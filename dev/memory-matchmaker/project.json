{
  "name": "memory-matchmaker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "implicitDependencies": ["socket-server"],
  "targets": {
    "setup": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "make build/e2e/bin/minimatch build/e2e/bin/default-evaluator build/e2e/bin/synchronizer"
        ]
      },
      "dependsOn": ["^build"]
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "cwd": ".",
        "main": "dev/memory-matchmaker/main.go"
      },
      "dependsOn": ["setup"]
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    }
  }
}
