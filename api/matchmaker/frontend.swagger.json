{
  "swagger": "2.0",
  "info": {
    "title": "matchmaker/frontend.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "Frontend"
    },
    {
      "name": "FrontendStream"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {},
  "definitions": {
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "matchmakerGameConfiguration": {
      "type": "object",
      "properties": {
        "gamemode": {
          "type": "string"
        }
      },
      "description": "GameConfiguration contains information for specifying a desired match."
    },
    "matchmakerGetStatusRequest": {
      "type": "object",
      "properties": {
        "target": {
          "type": "string"
        }
      }
    },
    "matchmakerMatchDetails": {
      "type": "object",
      "properties": {
        "match_id": {
          "type": "string"
        },
        "connection": {
          "type": "string"
        }
      },
      "description": "MatchDetails contains necessary match information to identify and connect to a match."
    },
    "matchmakerQueueDetails": {
      "type": "object",
      "properties": {
        "config": {
          "$ref": "#/definitions/matchmakerGameConfiguration"
        },
        "start_time": {
          "type": "string",
          "format": "date-time"
        }
      },
      "description": "QueueDetails contains information about a currently running queue."
    },
    "matchmakerQueueStopped": {
      "type": "object",
      "properties": {
        "message": {
          "type": "string"
        }
      },
      "description": "QueueStopped contains information about why a queue has stopped."
    },
    "matchmakerStartQueueRequest": {
      "type": "object",
      "properties": {
        "config": {
          "$ref": "#/definitions/matchmakerGameConfiguration"
        }
      }
    },
    "matchmakerState": {
      "type": "string",
      "enum": [
        "STATE_OFFLINE",
        "STATE_IDLE",
        "STATE_SEARCHING",
        "STATE_PLAYING"
      ],
      "default": "STATE_OFFLINE"
    },
    "matchmakerStreamResponse": {
      "type": "object",
      "properties": {
        "error": {
          "type": "string",
          "title": "error is sent whenever some stream request failed"
        },
        "status_update": {
          "$ref": "#/definitions/quipmatchmakerStatus"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "quipmatchmakerStatus": {
      "type": "object",
      "properties": {
        "state": {
          "$ref": "#/definitions/matchmakerState"
        },
        "searching": {
          "$ref": "#/definitions/matchmakerQueueDetails"
        },
        "stopped": {
          "$ref": "#/definitions/matchmakerQueueStopped"
        },
        "matched": {
          "$ref": "#/definitions/matchmakerMatchDetails"
        }
      },
      "description": "Status represents the current matchmaking status for any target."
    }
  }
}
