{
  "swagger": "2.0",
  "info": {
    "title": "quip-matchmaker.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "Matchmaker"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {},
  "definitions": {
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "quipMatchDetails": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "connection": {
          "type": "string"
        },
        "state": {
          "$ref": "#/definitions/quipMatchState"
        }
      }
    },
    "quipMatchState": {
      "type": "string",
      "enum": [
        "MATCH_PENDING",
        "MATCH_PLAYING",
        "MATCH_FINISHED"
      ],
      "default": "MATCH_PENDING",
      "description": " - MATCH_PENDING: Match has been allocated and is waiting for connections.\n - MATCH_PLAYING: Match is in progress.\n - MATCH_FINISHED: Match has completed."
    },
    "quipQueueDetails": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "gamemode": {
          "type": "string"
        },
        "start_time": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "quipStatus": {
      "type": "string",
      "enum": [
        "OFFLINE",
        "IDLE",
        "SEARCHING",
        "PLAYING"
      ],
      "default": "OFFLINE"
    },
    "quipStatusResponse": {
      "type": "object",
      "properties": {
        "status": {
          "$ref": "#/definitions/quipStatus"
        },
        "queue": {
          "$ref": "#/definitions/quipQueueDetails",
          "description": "Details about current queue status."
        },
        "match": {
          "$ref": "#/definitions/quipMatchDetails",
          "description": "Details about the match the user is currently playing in."
        }
      }
    }
  }
}
